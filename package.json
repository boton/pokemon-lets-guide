{
  "name": "pokemon-lets-guide",
  "version": "1.0.0",
  "displayName": "Pokémon Let's Guide",
  "description": "A Pokedex based in Pokémon Let's Go games",
  "main": "index.js",
  "repository": "git@github.com:lordfido/pokemon-lets-guide.git",
  "author": "Rafael Pérez García <rafael.perez@imperdiblesoft.com.com>",
  "license": "MIT",
  "lint-staged": {
    "linters": {
      "./src/**/*.js": [
        "npm run prettier",
        "npm run lint",
        "git add"
      ]
    }
  },
  "scripts": {
    "check-types": "tsc",
    "prettier": "prettier --config .prettierrc --write './src/**/*.tsx' './src/**/*.ts' './src/**/*.js'",
    "lint": "tslint src/**/*.ts src/**/*.tsx",
    "ts": "tsc",
    "clean:build": "rimraf dist",
    "pre-build": "npm run check-types && npm run lint && npm run prettier && npm run clean:build",
    "build:dev": "npm run pre-build && webpack --config ./webpack/dev.config.ts",
    "build:pre": "npm run pre-build && webpack --config ./webpack/pre.config.ts",
    "build": "npm run pre-build && webpack --config ./webpack/prod.config.ts",
    "serve:https": "webpack-dev-server --host 0.0.0.0 --https true --config ./webpack/dev.config.ts",
    "precommit": "lint-staged",
    "serve:dev": "webpack-dev-server --https true --host 0.0.0.0 --config ./webpack/dev.config.ts --cert=./cert/webapp-dev-server.crt --key=./cert/webapp-dev-server.key",
    "serve:pre": "webpack-dev-server --https true --host 0.0.0.0 --config ./webpack/pre.config.ts --cert=./cert/webapp-dev-server.crt --key=./cert/webapp-dev-server.key",
    "serve": "webpack-dev-server --https true --host 0.0.0.0 --config ./webpack/prod.config.ts --cert=./cert/webapp-dev-server.crt --key=./cert/webapp-dev-server.key",
    "start": "npm run serve:dev"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chroma-js": "^2.0.2",
    "classnames": "^2.2.6",
    "compare-versions": "^3.4.0",
    "history": "^4.7.2",
    "idb-keyval": "^3.1.0",
    "moment": "^2.22.2",
    "pokelab-lets-go": "^0.0.11",
    "pwacompat": "^2.0.7",
    "query-string": "^6.2.0",
    "rc-progress": "^2.2.6",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-lazy-img": "^1.0.2",
    "react-media": "^1.9.2",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.1.2",
    "react-svg-radar-chart": "^1.0.6",
    "react-textarea-autosize": "^7.1.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-async-to-generator": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@types/chroma-js": "^1.4.0",
    "@types/classnames": "^2.2.6",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/node": "^10.12.12",
    "@types/query-string": "^6.1.1",
    "@types/rc-progress": "^2.2.0",
    "@types/react": "^16.7.14",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-select": "^2.0.10",
    "@types/react-textarea-autosize": "^4.3.3",
    "@types/uglifyjs-webpack-plugin": "^1.1.0",
    "@types/uuid": "^3.4.4",
    "@types/webpack": "^4.4.21",
    "autoprefixer": "^9.4.2",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.0",
    "file-loader": "^2.0.0",
    "font-awesome": "^4.7.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.3",
    "image-webpack-loader": "^4.6.0",
    "lint-staged": "^8.0.4",
    "node-sass": "^4.11.0",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.15.3",
    "react-hot-loader": "^4.3.12",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.1.0",
    "ts-lint": "^4.5.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-pwa-manifest": "^4.0.0"
  }
}
